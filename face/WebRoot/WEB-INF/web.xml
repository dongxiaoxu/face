<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
     <servlet-name>upload</servlet-name>
     <servlet-class>site.dongxiaoxu.face.servlet.Upload</servlet-class>
 </servlet>
 
 <servlet-mapping>
     <servlet-name>upload</servlet-name>
     <url-pattern>/servlet/upload</url-pattern>
 </servlet-mapping> 
 
 <servlet>
 	<servlet-name>facecompare</servlet-name>
 	<servlet-class>site.dongxiaoxu.face.servlet.FaceCompare</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>facecompare</servlet-name>
 	<url-pattern>/face/facecompare</url-pattern>
 </servlet-mapping>
 
  <servlet>
 	<servlet-name>facedetect</servlet-name>
 	<servlet-class>site.dongxiaoxu.face.servlet.FaceDetect</servlet-class>
 </servlet>
  <servlet>
    <servlet-name>FileUploadTest</servlet-name>
    <servlet-class>site.dongxiaoxu.face.test.servlet.FileUploadTest</servlet-class>
  </servlet>

 <servlet-mapping>
 	<servlet-name>facedetect</servlet-name>
 	<url-pattern>/face/facedetect</url-pattern>
 </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUploadTest</servlet-name>
    <url-pattern>/test/FileUploadTest</url-pattern>
  </servlet-mapping>
 
 
</web-app>
